{
    "name": "craft-plugins/benchmark",
    "description": "Simple benchmark plugin with basic functionality for benchmarking.",
    "type": "craft-plugin",
    "keywords": ["twig", "craft", "craft-plugin", "craftcms", "benchmark"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Romanenko",
            "email": "awilum@msn.com",
            "homepage": "https://awilum.github.io"
        }
    ],
    "support": {
      "email": "awilum@msn.com",
      "issues": "https://github.com/craft-plugins/benchmark/issues?state=open",
      "source": "https://github.com/craft-plugins/benchmark",
      "docs": "https://github.com/craft-plugins/benchmark/master/README.md"
    },
    "require": {
      "craftcms/cms": "^4.3.3"
    },
    "autoload": {
      "psr-4": {
        "CraftPlugins\\Benchmark\\": "src/"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
      "name": "Benchmark",
      "handle": "benchmark",
      "class": "CraftPlugins\\Benchmark\\BenchmarkPLugin"
    },
    "require-dev": {
      "ext-iconv": "*",
      "phpstan/phpstan": "^1.8.0"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
